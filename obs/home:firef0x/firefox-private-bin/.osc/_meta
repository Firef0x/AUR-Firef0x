<package name="firefox-private-bin" project="home:firef0x">
  <title>firefox-private-bin</title>
  <description>Standalone web browser from mozilla.org, built with GCC -O3 SSE2, betterpgo, support for media-libs/gstreamer, and added an environment variable MOZILLA_DISABLE_PLUGINS to firefox.</description>
  <build>
    <enable arch="x86_64" repository="Arch_Extra"/>
    <disable arch="i586" repository="Arch_Extra"/>
    <disable arch="i586"/>
    <enable repository="Arch_Extra"/>
  </build>
  <publish>
    <disable arch="i586"/>
  </publish>
  <useforbuild>
    <disable arch="i586"/>
  </useforbuild>
</package>

